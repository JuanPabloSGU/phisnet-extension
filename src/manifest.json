{
  "manifest_version": 3,
  "name": "Phishnet Extension",
  "version": "1.0",
  "description": "A simple Chrome extension to connect to Phishnet",
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
      "storage"
  ],
  "host_permissions": [
    "http://localhost:5000/*"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}

